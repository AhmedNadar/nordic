        <section class="align-center map-info">
          <h2>Gothenburg</h2>
          <address>
            Trädgårdsgatan 3 <br>
            411 08 Göteborg,<br>
            Sweden<br>
            <a href="tel:+46317999040 ">+46-31-799-90-40 </a> <br>
            <a href="mailto:nordic@rsg.se">nordic@rsg.se</a>
          <address>
        </section>
      </div>

      <div class="flex-column twelve large-six">
        <figure class="brand">
          <a target="_blank" href="#" class="navbar-brand">
          <img src="<%= asset_path('S_map.png') %>" title="Stockholm Map" alt="Stockholm Map">
        </a>
        </figure>
        <section class="align-center map-info">
          <h2>Stockholm</h2>
          <address>
            Trädgårdsgatan 3 <br>
            411 08 Göteborg,<br>
            Sweden<br>
            <a href="tel:+46317999040 ">+46-31-799-90-40 </a> <br>
            <a href="mailto:nordic@rsg.se">nordic@rsg.se</a>
          <address>
        </section>
      </div>
    </section>
  </article>
